<form method="post" action="{{ path('enseignant_delete', {'id': enseignant.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet enseignant ? ');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ enseignant.id) }}">
    <center><button class="btn btn-primary">Supprimer</button></center>
</form>
